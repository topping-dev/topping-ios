<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.AppCompat.Empty" parent="" />
    <style name="Theme.AppCompat" parent="">
        <item name="android:windowBackground">#111111</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="iosToolbarTextColor">#FFFFFF</item>
        <item name="iosTheme">0</item>
    </style>
    <style name="Theme.AppCompat.Light" parent="">
        <item name="android:windowBackground">#FFFFFF</item>
        <item name="android:textColor">#000000</item>
        <item name="iosToolbarTextColor">#000000</item>
        <item name="iosTheme">1</item>
    </style>
    <style name="Theme.AppCompat.Light.DarkActionBar" parent="">
        <item name="android:windowBackground">#FFFFFF</item>
        <item name="android:textColor">#000000</item>
        <item name="iosToolbarTextColor">#FFFFFF</item>
        <item name="iosTheme">1</item>
    </style>
    <style name="Theme.AppCompat.NoActionBar" parent="Theme.AppCompat">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.AppCompat.Light.NoActionBar" parent="Theme.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.AppCompat.DialogWhenLarge"
           parent="Base.Theme.AppCompat.DialogWhenLarge">
    </style>
    <style name="Theme.AppCompat.Light.DialogWhenLarge"
           parent="Base.Theme.AppCompat.Light.DialogWhenLarge">
    </style>
    <style name="Theme.AppCompat.Dialog" parent="Base.Theme.AppCompat.Dialog" />
    <style name="Theme.AppCompat.Light.Dialog" parent="Base.Theme.AppCompat.Light.Dialog" />
    
    
    
    <style name="Theme.AppCompat.DayNight" parent="Theme.AppCompat.Light">
        <item name="iosTheme">-1</item>
    </style>

    <style name="Theme.AppCompat.DayNight.DarkActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="iosTheme">-1</item>
    </style>

    <style name="Theme.AppCompat.DayNight.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="iosTheme">-1</item>
    </style>

    <style name="Theme.AppCompat.DayNight.Dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="iosTheme">-1</item>
    </style>

    <style name="Theme.AppCompat.DayNight.Dialog.MinWidth" parent="Theme.AppCompat.Light.Dialog.MinWidth">
        <item name="iosTheme">-1</item>
    </style>

    <style name="Theme.AppCompat.DayNight.DialogWhenLarge" parent="Theme.AppCompat.Light.DialogWhenLarge">
        <item name="iosTheme">-1</item>
    </style>

    <style name="Theme.AppCompat.DayNight.Dialog.Alert" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="iosTheme">-1</item>
    </style>

    <style name="ThemeOverlay.AppCompat.DayNight" parent="ThemeOverlay.AppCompat.Light">
        <item name="iosTheme">-1</item>
    </style>

    <style name="ThemeOverlay.AppCompat.DayNight.ActionBar">
        <item name="colorControlNormal">?android:attr/textColorPrimary</item>
        <item name="searchViewStyle">@style/Widget.AppCompat.SearchView.ActionBar</item>
        <item name="iosTheme">-1</item>
    </style>
    
    <style name="Widget.ProgressBar.Horizontal">
        <item name="android:indeterminate">1</item>
        <item name="iosDarkProgress">0</item>
    </style>
    <style name="Widget.ProgressBar.Small">
        <item name="android:indeterminate">0</item>
        <item name="iosSmallProgress">1</item>
        <item name="iosDarkProgress">0</item>
    </style>
    <style name="Widget.ProgressBar.Large">
        <item name="android:indeterminate">0</item>
        <item name="iosSmallProgress">0</item>
        <item name="iosDarkProgress">0</item>
    </style>
    <style name="Widget.ProgressBar.Inverse" parent="Widget.ProgressBar.Horizontal">
        <item name="iosDarkProgress">1</item>
    </style>
    <style name="Widget.ProgressBar.Small.Inverse" parent="Widget.ProgressBar.Small">
        <item name="iosDarkProgress">1</item>
    </style>
    <style name="Widget.ProgressBar.Large.Inverse" parent="Widget.ProgressBar.Large">
        <item name="iosDarkProgress">1</item>
    </style>
</resources>

